<html>
    <head>
        <meta charset="UTF-8">
        <!-- Bootstrap 4.0.0-->
        <link rel='stylesheet' href='css/bootstrap.min.css'>
        <!-- Optional Theme from Bootstrap 4.0.0 -->
        <link rel="stylesheet" href="css/bootstrap-theme.min.css">
        <link rel="shortcut icon" type="image/png" href="png/favicon.ico">
    </head>
    <body>
        <div class='container'>
            <h1>
                Welcome to <%=title %>
            </h1>
            <div class='row'>
                <div class='col'>
                    <h2>SG1 Team Members</h2>
                    <ul class='list-group'>
                    <%for(var i=0; i<sg1.length; i++){%>
                        <li class='list-group-item'><%=sg1[i] %></li>
                    <%}%>
                    </ul>
                </div>
                <div class='col'>
                    <h2>Custom Team Members</h2>
                    <ul class='list-group'>
                        <li class='list-group-item'>Colonel</li>
                        <li class='list-group-item'>Captain</li>
                        <li class='list-group-item'>Support</li>
                        <li class='list-group-item'>Doctor</li>
                    </ul>
                </div>
            </div>
        </div>
        <!-- JQuery 3.3.1 -->
        <script src='js/jquery.min.js' type='application/javascript'></script>
        <!-- Bootstrap 4.0.0 -->
        <script src='js/bootstrap.min.js' type='application/javascript'></script>
    </body>
</html>